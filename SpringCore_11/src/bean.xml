 <bean id="ac1" class="com.test.Account">
 <constructor-arg name="an" value="9090"></constructor-arg>
 <constructor-arg name="aname" value="SBI"></constructor-arg>
 
 </bean>
 <bean id="ac2" class="com.test.Account">
 <constructor-arg name="an" value="9091"></constructor-arg>
 <constructor-arg name="aname" value="BOB"></constructor-arg>
 </bean>
 
 
