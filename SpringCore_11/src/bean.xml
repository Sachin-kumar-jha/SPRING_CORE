 <!--STANDALONE-->
 <util:list id="List" list-class="java.util.LinkedList">
  <ref bean="ac1"/>
  <ref bean="ac2"/>
 </util:list>
 
 
 
 <!--Seperate Bean tag for userDefined Class-->
 <bean id="list" class="java.util.LinkedList">

  <constructor-arg>
  <list>
  <ref bean="ac1"/>
  <ref bean="ac2"/>
  </list>
  </constructor-arg>
 </bean>
 
 
